<?xml version="1.0" encoding="UTF-8"?>
<addon id="plugin.video.nothingtoseehere" version="0.0.1" name="Nothing To See Here" provider-name="SomebodyThatIUsedToLove">
  <loglevel>2</loglevel>
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
    <import addon="script.module.resolveurl" version="5.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="main.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Nothing To See Here</summary>
    <description lang="en_GB">There is nothing to see here.</description>
    <disclaimer lang="en_GB">Seriously, there's nothing to see here</disclaimer>
    <assets>
      <icon>resources/icon.png</icon>
      <fanart>resources/fanart.png</fanart>
      <banner>resources/banner.png</banner>
      <poster>resources/poster.png</poster>
      <thumb>resources/thumbnail.png</thumb>
      <clearlogo>resources/clearlogo.png</clearlogo>
      <screenshot>resources/screenshot-01.jpg</screenshot>
      <screenshot>resources/screenshot-02.jpg</screenshot>
    </assets>
    <news>First release of absolutely nothing!</news>
    <settings src="resources/settings.xml"/>
  </extension>
  <extension point="xbmc.addon.repository">
    <dir>
      <info compressed="false">https://raw.githubusercontent.com/jsergio123/zips/master/addons.xml</info>
      <checksum>https://raw.githubusercontent.com/jsergio123/zips/master/addons.xml.md5</checksum>
      <datadir zip="true">https://raw.githubusercontent.com/jsergio123/zips/master/</datadir>
    </dir>
    <dir>
      <info compressed="false">https://somebodythatiusedtolove.github.io/NothingToRepoHere/addons.xml</info>
      <checksum>https://somebodythatiusedtolove.github.io/NothingToRepoHere/addons.xml.md5</checksum>
      <datadir zip="true">https://somebodythatiusedtolove.github.io/NothingToRepoHere/zips/</datadir>
    </dir>
  </extension>
</addon>
